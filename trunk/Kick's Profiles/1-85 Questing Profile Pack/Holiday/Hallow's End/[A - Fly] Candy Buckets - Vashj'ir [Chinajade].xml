			<CustomBehavior File="WaitTimer" WaitTime="500" VariantTime="2500" />
			<CustomBehavior File="Misc\RunLua" Lua="CloseLoot()" />
			<CustomBehavior File="WaitTimer" WaitTime="500" VariantTime="2500" />

