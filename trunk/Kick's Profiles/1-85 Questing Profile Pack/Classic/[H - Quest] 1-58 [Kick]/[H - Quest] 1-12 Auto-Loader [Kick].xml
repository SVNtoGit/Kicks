			<If Condition="Me.Race == WoWRace.Goblin" >
				<CustomBehavior File="LoadProfile" ProfileName="[H - Quest] 1-12 [Kick]\[H - Quest] 1-12 Goblin [Kick]" />
			</If>
			
			<If Condition="Me.Race == WoWRace.Orc" >
				<CustomBehavior File="LoadProfile" ProfileName="[H - Quest] 1-12 [Kick]\[H - Quest] 1-5 Orc [Kick]" />
			</If>
			
			<If Condition="Me.Race == WoWRace.Troll">
				<CustomBehavior File="LoadProfile" ProfileName="[H - Quest] 1-12 [Kick]\[H - Quest] 1-5 Troll [Kick]" />
			</If>
			
			<If Condition="Me.Race == WoWRace.Tauren" >
